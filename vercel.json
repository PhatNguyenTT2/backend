{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "admin/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/socket.io/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/admin/sitemap.xml"
    },
    {
      "src": "/robots.txt",
      "dest": "/admin/robots.txt"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|svg|woff|woff2|ttf|eot))",
      "dest": "/admin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/admin/index.html"
    }
  ]
}